{
    "compilerOptions": {
        "target": "ESNext",
        "useDefineForClassFields": true,
        "lib": [
            "DOM",
            "DOM.Iterable",
            "ESNext"
        ],
        "allowJs": false,
        "skipLibCheck": true,
        "esModuleInterop": false,
        "allowSyntheticDefaultImports": true,
        "strict": true,
        "forceConsistentCasingInFileNames": true,
        "module": "ESNext",
        "moduleResolution": "Node",
        "resolveJsonModule": true,
        "isolatedModules": false,
        "noEmit": true,
        "jsx": "react-jsx",
        "importHelpers": true,
        "experimentalDecorators": true,
        "sourceMap": true,
        "noImplicitAny": true,
        "baseUrl": ".",
        "paths": {
            "@/*": [
                "src/*"
            ]
        },
        "types": [
            "vite/client",
            "node"
        ]
    },
    "include": [
        "src/**/*.ts",
        "src/**/*.tsx",
        "antdResolve/*.ts",
        "*.d.ts",
    ],
    "exclude": [
        ".eslintrc.js"
    ],
    "references": [
        {
            "path": "./tsconfig.node.json"
        }
    ]
}